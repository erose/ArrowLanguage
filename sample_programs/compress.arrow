chunk_encode(ref chunk, const char){
    i := 0
    from i == 0 {
        chunk.left_del(char)
        i += 1
    } until chunk.len() == 0

    chunk += i.to_str()

    last_char := chunk.get(chunk.len()-1)
    i -= last_char.to_int()
    last_char == chunk.get(chunk.len()-1)

    chunk += char

    i == 0
}

main(data := "aaa"){
    chunk_encode(&data, "a")
}